<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/reaction_test_graph"
    app:startDestination="@id/testReactionInstruction">

    <fragment
        android:id="@+id/testReactionInstruction"
        android:name="com.example.psyhealthapp.user.testing.reaction.TestReactionInstructionFragment"
        android:label="InstructionFragment"
        tools:layout="@layout/test_reaction_instruction_fragment">
        <action
            android:id="@+id/action_testReactionInstruction_to_testReaction"
            app:destination="@id/testReaction" />
    </fragment>

    <fragment
        android:id="@+id/testReaction"
        android:name="com.example.psyhealthapp.user.testing.reaction.TestReactionFragment"
        android:label="ChallengeFragment"
        tools:layout="@layout/test_reaction_fragment">
        <action
            android:id="@+id/action_testReaction_to_tests_list"
            app:destination="@id/tests_list" />
    </fragment>
</navigation>